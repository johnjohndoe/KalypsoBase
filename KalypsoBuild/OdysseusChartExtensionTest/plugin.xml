            id="org.kalypso.chart.ext.test.EditableLineLayerProvider">
      </provider>
      <provider
            class="org.kalypso.chart.ext.test.layer.provider.BfgPegelLayerProvider"
            id="org.kalypso.chart.ext.test.BfgPegelLayerProvider">
      </provider>
      <provider
            class="org.kalypso.chart.ext.test.layer.provider.TortenLayerProvider"
            id="org.kalypso.chart.ext.test.TortenLayerProvider">
      </provider>
      <provider
            class="org.kalypso.chart.ext.test.layer.provider.BfgPegelDoubleLayerProvider"
            id="org.kalypso.chart.ext.test.BfgPegelDoubleLayerProvider">
      </provider>
      <provider
            class="org.kalypso.chart.ext.test.layer.provider.BfgPegelDoubleUnfilteredLayerProvider"
            id="org.kalypso.chart.ext.test.BfgPegelDoubleUnfilteredLayerProvider">
      </provider>
      <provider
            class="org.kalypso.chart.ext.test.layer.provider.BfgPegelUnfilteredLayerProvider"
