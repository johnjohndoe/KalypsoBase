      </provider>
      <provider
            class="org.kalypso.chart.ext.test.layer.provider.ColoredTestBarLayerProvider"
            id="org.kalypso.chart.ext.test.ColoredTestBarLayerProvider">
      </provider>
      <provider
            class="org.kalypso.chart.ext.test.layer.provider.TooltipTestLayerProvider"
            id="org.kalypso.chart.ext.test.TooltipTestLayerProvider">
      </provider>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="org.kalypso.chart.ext.test.view.InstalledExtensionView"
            id="de.openali.odysseus.chart.test.InstalledExtensionTestView"
            name="Installed Chart Extensions">
         <description>
            A view which displays all installed chart extensions
         </description>
      </view>
   </extension>


</plugin>
