<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.kalypso.contribs.java.urlCatalog">
      <catalog
            class="org.kalypso.calculation.chain.ModelConnectorUrlCatalog"
            id="org.kalypso.calculation.chain.ModelConnectorUrlCatalog">
      </catalog>
   </extension>
   <extension
         point="org.kalypso.simulation.core.simulation">
      <simulation
            catalogClass="org.kalypso.calculation.chain.ModelConnectorUrlCatalog"
            simulationClass="org.kalypso.calculation.chain.connector.na_wspm.Connector_NA_WSPM_Job"
            typeID="KalypsoModelConnector_NA_WSPM">
      </simulation>
      <simulation
            catalogClass="org.kalypso.calculation.chain.ModelConnectorUrlCatalog"
            simulationClass="org.kalypso.calculation.chain.connector.wspm_fm.Connector_WSPM_FM_Job"
            typeID="KalypsoModelConnector_WSPM_FM">
      </simulation>
      <simulation
            catalogClass="org.kalypso.calculation.chain.ModelConnectorUrlCatalog"
            simulationClass="org.kalypso.calculation.chain.connector.fm_rm.Connector_FM_RM_Job"
            typeID="KalypsoModelConnector_FM_RM">
      </simulation>
   </extension>
   <extension
         point="org.eclipse.core.runtime.adapters">
      <factory
            adaptableType="org.kalypsodeegree.model.feature.Feature"
            class="org.kalypso.calculation.chain.binding.CalculationChainAdapterFactory">
         <adapter
               type="org.kalypso.calculation.chain.binding.ICalculationChain">
         </adapter>
         <adapter
               type="org.kalypso.calculation.chain.binding.ICalculationChainMember">
         </adapter>
         <adapter
               type="org.kalypso.calculation.chain.binding.ICalculationChainMemberEntry">
         </adapter>
      </factory>
   </extension>
   <extension
         point="org.kalypso.deegree.featureBinding">
      <feature
            class="org.kalypso.calculation.chain.binding.CalculationChain"
            qname="{org.kalypso.calculation.chain}CalculationChain">
      </feature>
         <feature
            class="org.kalypso.calculation.chain.binding.CalculationChainMember"
            qname="{org.kalypso.calculation.chain}Calculation">
      </feature>
         <feature
            class="org.kalypso.calculation.chain.binding.CalculationChainMemberEntry"
            qname="{org.kalypso.calculation.chain}Entry">
      </feature>
   </extension>
</plugin>
