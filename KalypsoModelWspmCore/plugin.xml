<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
   <extension-point id="validatorrule" name="validatorrule" schema="schema/validatorrule.exsd"/>
   <extension-point id="profiletype" name="profiletype" schema="schema/profiletype.exsd"/>
   <extension-point id="profilserializer" name="profilserializer" schema="schema/profilserializer.exsd"/>
   <extension-point id="profileFeatureProvider" name="profileFeatureProvider" schema="schema/profileFeatureProvider.exsd"/>
   <extension-point id="profilePointFilter" name="profilePointFilter" schema="schema/profilePointFilter.exsd"/>
   <extension-point id="reparatorrule" name="reparatorrule" schema="schema/reparatorrule.exsd"/>
   <extension-point id="profilPointMarkerProvider" name="profilPointMarkerProvider" schema="schema/profilPointMarkerProvider.exsd"/>
   <extension-point id="profileObjectProvider" name="profileObjectProvider" schema="schema/profileObjectProvider.exsd"/>
   <extension
         point="org.kalypso.deegree.featureBinding">
      <feature
            class="org.kalypso.model.wspm.core.gml.ProfileFeatureBinding"
            qname="{org.kalypso.model.wspmprofile}Profile">
      </feature>
      <feature
            class="org.kalypso.model.wspm.core.gml.WspmWaterBody"
            qname="{org.kalypso.model.wspm}WaterBody">
      </feature>
      <feature
            class="org.kalypso.model.wspm.core.gml.WspmFixation"
            qname="{org.kalypso.model.wspmrunoff}WaterlevelFixation">
      </feature>
      <feature
            class="org.kalypso.model.wspm.core.gml.RunOffEvent"
            qname="{org.kalypso.model.wspmrunoff}RunOffEvent">
      </feature>
      <feature
            class="org.kalypso.model.wspm.core.gml.classifications.WspmClassification"
            qname="{org.kalypso.model.wspm.classifications}WspmClassification">
      </feature>
      <feature
            class="org.kalypso.model.wspm.core.gml.classifications.RoughnessClass"
            qname="{org.kalypso.model.wspm.classifications}RoughnessClass">
      </feature>
      <feature
            class="org.kalypso.model.wspm.core.gml.classifications.VegetationClass"
            qname="{org.kalypso.model.wspm.classifications}VegetationClass">
      </feature>
      <feature
            class="org.kalypso.model.wspm.core.gml.classifications.CodeClass"
            qname="{org.kalypso.model.wspm.classifications}CodeClass">
      </feature>
   </extension>
   <extension
           point="org.eclipse.core.runtime.preferences">
        <initializer class="org.kalypso.model.wspm.core.gml.validation.ValidationPreferenceInitializer"/>
   </extension>
</plugin>
