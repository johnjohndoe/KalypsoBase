<?xml version="1.0" encoding="UTF-8"?>
<ZmlTable xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="schema.table.core.zml.kalypso.org">

  <defaultStyle>
    <url>urn:org:kalypso:zml:ui:table:global#defaultCellStyle</url>
  </defaultStyle>

  <toolbar>toolbar:org.kalypso.hwv.product.sachsenanhalt.zml.table.menu.toolbar.instantaneous</toolbar>

  <Columns>
    <!-- date column -->
    <IndexColumn autopack="true" id="date">
      <property name="label">Datum</property>
      <property name="alignment">LEFT</property>

      <defaultCellStyle>
        <url>urn:org:kalypso:zml:ui:table:global#dateCellStyle</url>
      </defaultCellStyle>

      <rule>
        <url>urn:org:kalypso:zml:ui:table:global#ruleForecast</url>
      </rule>
    </IndexColumn>


    <!-- wasserstand -->
    <DataColumn  editable="true" id="W_MESS" metadataSource="true">
      <property name="label">Wasserstand (Messung)</property>
      <property name="width">150</property>
      <property name="alignment">RIGHT</property>
      <property name="uriContextMenu">menu:org.kalypso.hwv.product.sachsenanhalt.zml.table.menu.cell.ContextMenu</property>
      <property name="uriHeaderContextMenu">menu:org.kalypso.hwv.product.sachsenanhalt.zml.table.menu.header.ContextMenu</property>

      <defaultCellStyle>
        <url>urn:org:kalypso:zml:ui:table:global#wBaseCellStyle</url>
      </defaultCellStyle>
      <defaultCellEditingStyle>
        <url>urn:org:kalypso:zml:ui:table:global#wBaseEditingCellStyle</url>
      </defaultCellEditingStyle>

      <rule>
        <url>urn:org:kalypso:zml:ui:table:global#ruleUserModified</url>
      </rule>
      <rule>
        <url>urn:org:kalypso:zml:ui:table:global#ruleInterpolatedValue</url>
      </rule>
      <rule>
        <url>urn:org:kalypso:zml:ui:table:global#ruleWqConflict</url>
      </rule>
      <rule>
        <url>urn:org:kalypso:zml:ui:table:global#ruleAlarmstufen</url>
      </rule>
      <rule>
        <url>urn:org:kalypso:zml:ui:table:global#ruleHauptzahlenW</url>
      </rule>

      <indexAxis>date</indexAxis>
      <valueAxis>W</valueAxis>
    </DataColumn>

    <!-- wasserstand modell zeitreihe -->
    <DataColumn  editable="true" id="W_MODELL">
      <property name="label">Wasserstand (Modell)</property>
      <property name="width">150</property>
      <property name="alignment">RIGHT</property>
      <property name="uriContextMenu">menu:org.kalypso.hwv.product.sachsenanhalt.zml.table.menu.cell.ContextMenu</property>
      <property name="uriHeaderContextMenu">menu:org.kalypso.hwv.product.sachsenanhalt.zml.table.menu.header.ContextMenu</property>

      <defaultCellStyle>
        <url>urn:org:kalypso:zml:ui:table:global#wBaseCellStyle</url>
      </defaultCellStyle>
      <defaultCellEditingStyle>
        <url>urn:org:kalypso:zml:ui:table:global#wBaseEditingCellStyle</url>
