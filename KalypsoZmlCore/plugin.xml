<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="tableRule" name="tableRule" schema="schema/tableRule.exsd"/>
  	<extension
       point="org.kalypso.zml.core.tableRule">
    <rule
          id="org.kalypso.zml.ui.core.rule.user.modified"
          rule="org.kalypso.zml.core.table.rules.impl.ZmlRuleUserModified">
    </rule>
    <rule
          id="org.kalypso.zml.ui.core.rule.value.interpolated"
          rule="org.kalypso.zml.core.table.rules.impl.ZmlRuleInterpolatedValue">
    </rule>
    <rule
          id="org.kalypso.zml.ui.core.rule.value.interval"
          rule="org.kalypso.zml.core.table.rules.impl.ZmlRuleIntervalValue">
    </rule>
    <rule
          id="org.kalypso.zml.ui.core.rule.warned.value"
          rule="org.kalypso.zml.core.table.rules.impl.ZmlRuleWarnedValue">
    </rule>
    <rule
          id="org.kalypso.zml.ui.core.rule.wq.conflict"
          rule="org.kalypso.zml.core.table.rules.impl.ZmlRuleWQRelationConflict">
    </rule>
    <rule
           id="org.kalypso.zml.ui.core.rule.grenzwerte"
           rule="org.kalypso.zml.core.table.rules.impl.grenzwert.ZmlRuleGrenzwerte">
    </rule>
    <rule
           id="org.kalypso.zml.ui.core.rule.derived.value"
           rule="org.kalypso.zml.core.table.rules.impl.ZmlDerivedValue">
    </rule>
    <rule
           id="org.kalypso.zml.ui.core.rule.forecast.value"
           rule="org.kalypso.zml.core.table.rules.impl.forecast.ZmlRuleForecast">
    </rule>
 </extension> 
 <extension
         point="org.eclipse.wst.xml.core.catalogContributions">
      <catalogContribution>
         <nextCatalog
               catalog="etc/schemas/catalog.xmlcatalog">
         </nextCatalog>
      </catalogContribution>
 </extension>

</plugin>
