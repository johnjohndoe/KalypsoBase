<?xml version="1.0" encoding="UTF-8"?>
<zml:ZmlTable xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:zml="schema.table.ui.zml.kalypso.org" xsi:schemaLocation="schema.table.ui.zml.kalypso.org zmltable.xsd ">

  <zml:StyleSet>
    <zml:Style id="baseCellStyle" default="true">
      <zml:Property name="backgroundColor">FFFFFF</zml:Property>
      <zml:Property name="textColor">000000</zml:Property>
      <zml:Property name="fontFamily">Arial</zml:Property>
    </zml:Style>
    <zml:Style styleRef="baseCellStyle" id="userModifiedStyle">
      <zml:Property name="icon">urn:org:kalypso:zml:ui:table:rule:user:modified:icon</zml:Property>
    </zml:Style>
    <zml:Style styleRef="baseCellStyle" id="interpolatedValueStyle">
      <zml:Property name="icon">urn:org:kalypso:zml:ui:table:rule:interpolated:value:icon</zml:Property>
    </zml:Style>
    <zml:Style styleRef="baseCellStyle" id="wqConflictStyle">
      <zml:Property name="icon">urn:org:kalypso:zml:ui:table:rule:wq:conflict:icon</zml:Property>
    </zml:Style>
  </zml:StyleSet>

  <zml:Columns>
    <zml:Column xsi:type="zml:IndexColumnType" autopack="true" id="date">
      <zml:Property name="label">Datum</zml:Property>
      <zml:Property name="format">dd.MM.yyyy HH:mm</zml:Property>
      <zml:Property name="alignment">LEFT</zml:Property>

      <zml:Style id="dateStyle">
        <zml:Property name="backgroundColor">A2B5DB</zml:Property>
      </zml:Style>
    </zml:Column>

    <zml:Column xsi:type="zml:DataColumnType" editable="true" id="W">
      <zml:Property name="label">Wasserstand</zml:Property>
      <zml:Property name="format">%.0f</zml:Property>
      <zml:Property name="width">100</zml:Property>
      <zml:Property name="alignment">RIGHT</zml:Property>


      <zml:Rules>
        <zml:Rule ruleReference="org.kalypso.zml.ui.table.rule.user.modified" styleReference="userModifiedStyle" />
        <zml:Rule ruleReference="org.kalypso.zml.ui.table.rule.value.interpolated" styleReference="interpolatedValueStyle" />
        <zml:Rule ruleReference="org.kalypso.zml.ui.table.rule.wq.conflict" styleReference="wqConflictStyle" />
      </zml:Rules>

      <zml:indexAxis>date</zml:indexAxis>
      <zml:valueAxis>W</zml:valueAxis>
    </zml:Column>

    <zml:Column xsi:type="zml:DataColumnType" editable="true" id="W_MODELL">
      <zml:Property name="label">Wasserstand (Modell)</zml:Property>
      <zml:Property name="format">%.0f</zml:Property>
      <zml:Property name="width">100</zml:Property>
      <zml:Property name="alignment">RIGHT</zml:Property>

      <zml:Rules>
        <zml:Rule ruleReference="org.kalypso.zml.ui.table.rule.user.modified" styleReference="userModifiedStyle" />
        <zml:Rule ruleReference="org.kalypso.zml.ui.table.rule.value.interpolated" styleReference="interpolatedValueStyle" />
        <zml:Rule ruleReference="org.kalypso.zml.ui.table.rule.wq.conflict" styleReference="wqConflictStyle" />
      </zml:Rules>

      <zml:indexAxis>date</zml:indexAxis>
      <zml:valueAxis>W</zml:valueAxis>
    </zml:Column>

    <zml:Column xsi:type="zml:DataColumnType" editable="true" id="Q">
      <zml:Property name="label">Abfluss</zml:Property>
      <zml:Property name="format">%.3f</zml:Property>
      <zml:Property name="width">100</zml:Property>
      <zml:Property name="alignment">RIGHT</zml:Property>

      <zml:Style id="qCellStyle" styleRef="baseCellStyle">
        <zml:Property name="backgroundColor">F8F3C0</zml:Property>
      </zml:Style>

      <zml:Rules>
        <zml:Rule ruleReference="org.kalypso.zml.ui.table.rule.user.modified" styleReference="userModifiedStyle" />
        <zml:Rule ruleReference="org.kalypso.zml.ui.table.rule.value.interpolated" styleReference="interpolatedValueStyle" />
        <zml:Rule ruleReference="org.kalypso.zml.ui.table.rule.wq.conflict" styleReference="wqConflictStyle" />
      </zml:Rules>

      <zml:indexAxis>date</zml:indexAxis>
      <zml:valueAxis>Q</zml:valueAxis>
    </zml:Column>

    <zml:Column xsi:type="zml:DataColumnType" editable="true" id="Q_MODELL">
      <zml:Property name="label">Abfluss (Modell)</zml:Property>
      <zml:Property name="format">%.3f</zml:Property>
      <zml:Property name="width">100</zml:Property>
      <zml:Property name="alignment">RIGHT</zml:Property>

      <zml:Style id="qModellCellStyle" styleRef="baseCellStyle">
        <zml:Property name="backgroundColor">F8F3C0</zml:Property>
      </zml:Style>

      <zml:Rules>
        <zml:Rule ruleReference="org.kalypso.zml.ui.table.rule.user.modified" styleReference="userModifiedStyle" />
        <zml:Rule ruleReference="org.kalypso.zml.ui.table.rule.value.interpolated" styleReference="interpolatedValueStyle" />
        <zml:Rule ruleReference="org.kalypso.zml.ui.table.rule.wq.conflict" styleReference="wqConflictStyle" />
      </zml:Rules>

      <zml:indexAxis>date</zml:indexAxis>
      <zml:valueAxis>Q</zml:valueAxis>
    </zml:Column>

  </zml:Columns>
</zml:ZmlTable>
