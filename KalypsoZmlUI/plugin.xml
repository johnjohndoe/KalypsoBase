<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="tableRule" name="tableRule" schema="schema/tableRule.exsd"/>
    <extension
       point="de.openali.odysseus.chart.factory.LayerProvider">
    <provider
          class="org.kalypso.zml.ui.chart.layer.provider.ZmlLineLayerProvider"
          id="org.kalypso.zml.ui.chart.layer.provider.ZmlLineLayerProvider">
    </provider>
    <provider
          class="org.kalypso.zml.ui.chart.layer.provider.ZmlGridLayerProvider"
          id="org.kalypso.zml.ui.chart.layer.provider.ZmlGridLayerProvider">
    </provider>
    <provider
          class="org.kalypso.zml.ui.chart.layer.provider.ZmlSinglePointProvider"
          id="org.kalypso.zml.ui.chart.layer.provider.ZmlSinglePointProvider">
    </provider>
 </extension>
	<extension
         point="org.eclipse.wst.xml.core.catalogContributions">
      <catalogContribution>
         <nextCatalog
               catalog="etc/schemas/catalog.xmlcatalog">
         </nextCatalog>
      </catalogContribution>
   </extension>
 <extension
       point="org.kalypso.zml.ui.tableRule">
    <rule
          id="org.kalypso.zml.ui.table.rule.user.modified"
          rule="org.kalypso.zml.ui.table.rules.impl.ZmlRuleUserModified">
    </rule>
      <rule
          id="org.kalypso.zml.ui.table.rule.value.interpolated"
          rule="org.kalypso.zml.ui.table.rules.impl.ZmlRuleInterpolatedValue">
    </rule>
     <rule
          id="org.kalypso.zml.ui.table.rule.wq.conflict"
          rule="org.kalypso.zml.ui.table.rules.impl.ZmlRuleWQRelationConflict">
    </rule>
     <rule
           id="org.kalypso.zml.ui.table.rule.forecast.value"
           rule="org.kalypso.zml.ui.table.rules.impl.ZmlRuleForecastValue">
     </rule>
 </extension> 
 
 <extension point="org.kalypso.core.catalogContribution">
    <catalogContribution class="org.kalypso.zml.ui.table.rules.ZmlTableRulesCatalogContribution"
      name="org.kalypso.zml.ui.table.rules.ZmlTableRulesCatalogContributionbution" />
  </extension>
  
  
 <extension point="org.eclipse.ui.commands">
   <category id="org.kalypso.zml.ui.table.commands.category"
      name="zml table commands - category">
   </category>
 </extension>
    
 <extension point="org.eclipse.ui.contexts">
	<context id="org.kalypso.zml.ui.table.context" name="Table" parentId="org.eclipse.ui.contexts.window" />
 </extension>
</plugin>
