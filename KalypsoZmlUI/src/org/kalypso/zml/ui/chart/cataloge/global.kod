<?xml version="1.0" encoding="UTF-8"?>
<ChartConfiguration xmlns="http://www.openali.de/odysseus/chartconfig/0.2.0/">
  <Chart id="global">
    <Styles>
      <!-- TextStyles -->
      <TextStyle role="axisLabel">
        <Size>8</Size>
        <TextColor>000000</TextColor>
        <FontFamily>Arial</FontFamily>
        <FontWeight>NORMAL</FontWeight>
        <FontStyle>NORMAL</FontStyle>
      </TextStyle>
      <TextStyle role="tickLabel">
        <Size>8</Size>
        <TextColor>000000</TextColor>
        <FontFamily>Arial</FontFamily>
        <FontWeight>NORMAL</FontWeight>
        <FontStyle>NORMAL</FontStyle>
      </TextStyle>
      <TextStyle role="header">
        <Size>10</Size>
        <TextColor>000000</TextColor>
        <FontFamily>Arial</FontFamily>
        <FontWeight>NORMAL</FontWeight>
        <FontStyle>NORMAL</FontStyle>
      </TextStyle>
      <!-- LineStyles W (blue) -->
      <LineStyle role="w_line_0">
        <Width>2</Width>
        <LineColor>8FADDF</LineColor>
      </LineStyle>
      <LineStyle role="w_active_line_0">
        <Width>3</Width>
        <LineColor>8FADDF</LineColor>
      </LineStyle>
      <LineStyle role="w_line_1">
        <Width>2</Width>
        <LineColor>3460A9</LineColor>
      </LineStyle>
      <LineStyle role="w_active_line_1">
        <Width>3</Width>
        <LineColor>3460A9</LineColor>
      </LineStyle>
      <LineStyle role="w_line_2">
        <Width>2</Width>
        <LineColor>BDD3F6</LineColor>
      </LineStyle>
      <LineStyle role="w_active_line_2">
        <Width>3</Width>
        <LineColor>BDD3F6</LineColor>
      </LineStyle>
      <LineStyle role="w_line_3">
        <Width>2</Width>
        <LineColor>093689</LineColor>
      </LineStyle>
      <LineStyle role="w_active_line_3">
        <Width>3</Width>
        <LineColor>093689</LineColor>
      </LineStyle>
      <LineStyle role="w_line_4">
        <Width>2</Width>
        <LineColor>6186C3</LineColor>
      </LineStyle>
      <LineStyle role="w_active_line_4">
        <Width>3</Width>
        <LineColor>6186C3</LineColor>
      </LineStyle>
      <!-- LineStyles W_NN -->
      <LineStyle role="w_nn_line_0">
        <Width>2</Width>
        <LineColor>000080</LineColor>
      </LineStyle>
      <!-- LineStyles Q (greenish) -->
      <LineStyle role="q_line_0">
        <Width>2</Width>
        <LineColor>ABB985</LineColor>
      </LineStyle>
      <LineStyle role="q_active_line_0">
        <Width>3</Width>
        <LineColor>ABB985</LineColor>
      </LineStyle>
      <LineStyle role="q_line_1">
        <Width>2</Width>
        <LineColor>65782F</LineColor>
      </LineStyle>
      <LineStyle role="q_active_line_1">
        <Width>3</Width>
        <LineColor>65782F</LineColor>
      </LineStyle>
      <LineStyle role="q_line_2">
        <Width>2</Width>
        <LineColor>CFDAB2</LineColor>
      </LineStyle>
      <LineStyle role="q_active_line_2">
        <Width>3</Width>
        <LineColor>CFDAB2</LineColor>
      </LineStyle>
      <LineStyle role="q_line_3">
        <Width>2</Width>
        <LineColor>3D5208</LineColor>
      </LineStyle>
      <LineStyle role="q_active_line_3">
        <Width>3</Width>
        <LineColor>3D5208</LineColor>
      </LineStyle>
      <LineStyle role="q_line_4">
        <Width>2</Width>
        <LineColor>879859</LineColor>
      </LineStyle>
      <LineStyle role="q_active_line_4">
        <Width>3</Width>
        <LineColor>879859</LineColor>
      </LineStyle>
      <!-- LinesStyles V -->
      <LineStyle role="v_line_0">
        <Width>2</Width>
        <LineColor>BDAB7A</LineColor>
      </LineStyle>
      <LineStyle role="v_line_1">
        <Width>2</Width>
        <LineColor>769844</LineColor>
      </LineStyle>
      <LineStyle role="v_line_2">
        <Width>2</Width>
        <LineColor>DFD0A7</LineColor>
      </LineStyle>
      <LineStyle role="v_line_3">
        <Width>2</Width>
        <LineColor>4D3907</LineColor>
      </LineStyle>
      <LineStyle role="v_line_4">
        <Width>2</Width>
        <LineColor>998652</LineColor>
      </LineStyle>
      <!-- AreaStyles N (bluish) -->
      <AreaStyle role="n_area_0">
        <Alpha>64</Alpha>
        <Fill>
          <ColorFill>
            <Color>093689</Color>
          </ColorFill>
        </Fill>
        <Stroke>
          <Width>1</Width>
          <LineColor>093689</LineColor>
        </Stroke>
      </AreaStyle>
      <AreaStyle role="n_area_1">
        <Alpha>64</Alpha>
        <Fill>
          <ColorFill>
            <Color>6186C3</Color>
          </ColorFill>
        </Fill>
        <Stroke>
          <Width>1</Width>
          <LineColor>6186C3</LineColor>
        </Stroke>
      </AreaStyle>
      <AreaStyle role="n_area_2">
        <Alpha>64</Alpha>
        <Fill>
          <ColorFill>
            <Color>008000</Color>
          </ColorFill>
        </Fill>
        <Stroke>
          <Width>1</Width>
          <LineColor>008000</LineColor>
        </Stroke>
      </AreaStyle>
      <AreaStyle role="n_area_3">
        <Alpha>64</Alpha>
        <Fill>
          <ColorFill>
            <Color>FF8000</Color>
          </ColorFill>
        </Fill>
        <Stroke>
          <Width>1</Width>
          <LineColor>FF8000</LineColor>
        </Stroke>
      </AreaStyle>
      <AreaStyle role="n_area_4">
        <Alpha>64</Alpha>
        <Fill>
          <ColorFill>
            <Color>FF00FF</Color>
          </ColorFill>
        </Fill>
        <Stroke>
          <Width>1</Width>
          <LineColor>FF00FF</LineColor>
        </Stroke>
      </AreaStyle>
      <AreaStyle role="n_area_5">
        <Alpha>64</Alpha>
        <Fill>
          <ColorFill>
            <Color>000040</Color>
          </ColorFill>
        </Fill>
        <Stroke>
          <Width>1</Width>
          <LineColor>000040</LineColor>
        </Stroke>
      </AreaStyle>
      <!-- -->
      <PointStyle role="invisible_point" isVisible="false" />
    </Styles>
    <Layers>
      <Layer id="W" visible="true" legend="false" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
        <Title>Wasserstand</Title>
        <Layers>
          <Layer id="Wasserstand" visible="true" legend="true" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
            <Provider epid="org.kalypso.zml.ui.chart.layer.provider.ZmlLineLayerProvider">
              <Parameters>
                <Parameter name="supportsMultiSelect" value="true" />
              </Parameters>
            </Provider>
            <Title>Wasserstand</Title>
            <Description />
            <Styles>
              <StyleReference>urn:org:kalypso:zml:ui:diagramm:global#w_line_0</StyleReference>
              <StyleReference>urn:org:kalypso:zml:ui:diagramm:global#w_line_1</StyleReference>
              <StyleReference>urn:org:kalypso:zml:ui:diagramm:global#w_line_2</StyleReference>
              <StyleReference>urn:org:kalypso:zml:ui:diagramm:global#w_line_3</StyleReference>
              <StyleReference>urn:org:kalypso:zml:ui:diagramm:global#w_line_4</StyleReference>
              <StyleReference>urn:org:kalypso:zml:ui:diagramm:global#invisible_point</StyleReference>
            </Styles>
          </Layer>
          <LayerReference url="urn:org:kalypso:zml:ui:diagramm:grid#grid" />
          <LayerReference url="urn:org:kalypso:zml:ui:diagramm:hauptzahlen#hauptzahlen" />
          <LayerReference url="urn:org:kalypso:zml:ui:diagramm:alarmstufen#meldegrenze" />
          <LayerReference url="urn:org:kalypso:zml:ui:diagramm:alarmstufen#alarmstufe_1" />
          <LayerReference url="urn:org:kalypso:zml:ui:diagramm:alarmstufen#alarmstufe_2" />
          <LayerReference url="urn:org:kalypso:zml:ui:diagramm:alarmstufen#alarmstufe_3" />
          <LayerReference url="urn:org:kalypso:zml:ui:diagramm:alarmstufen#alarmstufe_4" />
        </Layers>
        <MapperRefs>
          <DomainAxisRef ref="date" />
          <TargetAxisRef ref="W" />
        </MapperRefs>
      </Layer>
      <Layer id="Q">
        <Title>Abfluss</Title>
        <Layers>
          <Layer id="Abfluss" visible="true" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
            <Provider epid="org.kalypso.zml.ui.chart.layer.provider.ZmlLineLayerProvider">
            </Provider>
            <Title>Abfluss</Title>
            <Description />
            <Styles>
              <StyleReference>urn:org:kalypso:zml:ui:diagramm:global#q_line_0</StyleReference>
              <StyleReference>urn:org:kalypso:zml:ui:diagramm:global#q_line_1</StyleReference>
              <StyleReference>urn:org:kalypso:zml:ui:diagramm:global#q_line_2</StyleReference>
              <StyleReference>urn:org:kalypso:zml:ui:diagramm:global#q_line_3</StyleReference>
              <StyleReference>urn:org:kalypso:zml:ui:diagramm:global#q_line_4</StyleReference>
              <StyleReference>urn:org:kalypso:zml:ui:diagramm:global#invisible_point</StyleReference>
            </Styles>
          </Layer>
          <LayerReference url="urn:org:kalypso:zml:ui:diagramm:grid#grid" />
          <LayerReference url="urn:org:kalypso:zml:ui:diagramm:hauptzahlen#hauptzahlen" />
          <LayerReference url="urn:org:kalypso:zml:ui:diagramm:alarmstufen#meldegrenze" />
          <LayerReference url="urn:org:kalypso:zml:ui:diagramm:alarmstufen#alarmstufe_1" />
          <LayerReference url="urn:org:kalypso:zml:ui:diagramm:alarmstufen#alarmstufe_2" />
          <LayerReference url="urn:org:kalypso:zml:ui:diagramm:alarmstufen#alarmstufe_3" />
          <LayerReference url="urn:org:kalypso:zml:ui:diagramm:alarmstufen#alarmstufe_4" />
        </Layers>
        <MapperRefs>
          <DomainAxisRef ref="date" />
          <TargetAxisRef ref="Q" />
        </MapperRefs>
      </Layer>
      <Layer id="N">
        <Title>Niederschlag</Title>
        <Layers>
          <Layer id="Niederschlag" visible="true" legend="true" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
            <Provider epid="org.kalypso.zml.ui.chart.layer.provider.ZmlBarLayerProvider">
            </Provider>
            <Title>Niederschlag</Title>
            <Description />
            <Styles>
              <StyleReference>urn:org:kalypso:zml:ui:diagramm:global#n_area_0</StyleReference>
              <StyleReference>urn:org:kalypso:zml:ui:diagramm:global#n_area_1</StyleReference>
              <StyleReference>urn:org:kalypso:zml:ui:diagramm:global#n_area_2</StyleReference>
              <StyleReference>urn:org:kalypso:zml:ui:diagramm:global#n_area_3</StyleReference>
              <StyleReference>urn:org:kalypso:zml:ui:diagramm:global#n_area_4</StyleReference>
              <StyleReference>urn:org:kalypso:zml:ui:diagramm:global#invisible_point</StyleReference>
            </Styles>
          </Layer>
          <LayerReference url="urn:org:kalypso:zml:ui:diagramm:grid#grid" />
        </Layers>
        <MapperRefs>
          <DomainAxisRef ref="date" />
          <TargetAxisRef ref="N" />
        </MapperRefs>
      </Layer>
      <Layer id="W_NN">
        <Title>(Wasserstands)Höhe</Title>
        <Layers>
          <Layer id="Wasserstand_mNN" visible="true" legend="true" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
            <Provider epid="org.kalypso.zml.ui.chart.layer.provider.ZmlLineLayerProvider" />
            <Title>(Wasserstands)Höhe</Title>
            <Description />
            <Styles>
              <StyleReference>urn:org:kalypso:zml:ui:diagramm:global#w_nn_line_0</StyleReference>
              <StyleReference>urn:org:kalypso:zml:ui:diagramm:global#invisible_point</StyleReference>
            </Styles>
          </Layer>
          <LayerReference url="urn:org:kalypso:zml:ui:diagramm:grid#grid" />
        </Layers>
        <MapperRefs>
          <DomainAxisRef ref="date" />
          <TargetAxisRef ref="W_NN" />
        </MapperRefs>
      </Layer>
      <Layer id="V">
        <Title>Volumen</Title>
        <Layers>
          <Layer id="Volumen" visible="true" legend="true" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
            <Provider epid="org.kalypso.zml.ui.chart.layer.provider.ZmlLineLayerProvider" />
            <Title>Volumen</Title>
            <Description />
            <Styles>
              <StyleReference>urn:org:kalypso:zml:ui:diagramm:global#v_line_0</StyleReference>
              <StyleReference>urn:org:kalypso:zml:ui:diagramm:global#v_line_1</StyleReference>
              <StyleReference>urn:org:kalypso:zml:ui:diagramm:global#v_line_2</StyleReference>
              <StyleReference>urn:org:kalypso:zml:ui:diagramm:global#v_line_3</StyleReference>
              <StyleReference>urn:org:kalypso:zml:ui:diagramm:global#v_line_4</StyleReference>
              <StyleReference>urn:org:kalypso:zml:ui:diagramm:global#invisible_point</StyleReference>
            </Styles>
          </Layer>
          <LayerReference url="urn:org:kalypso:zml:ui:diagramm:grid#grid" />
        </Layers>
        <MapperRefs>
          <DomainAxisRef ref="date" />
          <TargetAxisRef ref="V" />
        </MapperRefs>
      </Layer>
      <Layer id="forecastLayer" visible="true" legend="false" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
        <Provider epid="org.kalypso.zml.ui.chart.layer.provider.ZmlForecastLayerProvider">
          <Parameters>
            <Parameter name="hideOnMultiSelect" value="true" />
          </Parameters>
        </Provider>
        <Title>Vorhersagezeitraum</Title>
        <Description />
        <MapperRefs>
          <DomainAxisRef ref="date" />
          <TargetAxisRef ref="screenAxisY" />
        </MapperRefs>
      </Layer>
      <Layer id="noData" visible="true" legend="false" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
        <Provider epid="de.openali.odysseus.chart.ext.base.layer.provider.DefaultTextLayerProvider">
          <Parameters>
            <Parameter name="text" value="Keine Daten vorhanden" />
            <Parameter name="alignment.horizontal" value="CENTERED_HORIZONTAL" />
            <Parameter name="alignment.vertical" value="CENTERED_VERTICAL" />
            <Parameter name="text.anchor.x" value="CENTERED_HORIZONTAL" />
            <Parameter name="text.anchor.y" value="CENTERED_VERTICAL" />
          </Parameters>
        </Provider>
        <Title>Keine Daten vorhanden</Title>
        <Description />
        <MapperRefs>
          <DomainAxisRef ref="screenAxisX" />
          <TargetAxisRef ref="screenAxisY" />
        </MapperRefs>
        <Styles>
          <TextStyle role="defaultTextStyle">
            <Size>16</Size>
            <TextColor>000000</TextColor>
            <FontWeight>BOLD</FontWeight>
            <FontStyle>NORMAL</FontStyle>
          </TextStyle>
        </Styles>
      </Layer>
    </Layers>
    <Mappers>
      <Axis id="W" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
        <Provider epid="de.openali.odysseus.chart.ext.axis.provider.GenericLinear" />
        <Label>Wasserstand [cm]</Label>
        <Direction>POSITIVE</Direction>
        <Position>LEFT</Position>
        <PreferredAdjustment before="15" range="75" after="10" />
        <RendererRef ref="AutoNumberAxisRenderer" />
      </Axis>
      <Axis id="Q" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
        <Provider epid="de.openali.odysseus.chart.ext.axis.provider.GenericLinear" />
        <Label>Abfluss [m³]</Label>
        <Direction>POSITIVE</Direction>
        <Position>LEFT</Position>
        <PreferredAdjustment before="15" range="75" after="10" />
        <RendererRef ref="AutoNumberAxisRenderer" />
      </Axis>
      <Axis id="N" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
        <Provider epid="de.openali.odysseus.chart.ext.axis.provider.GenericLinear" />
        <Label>Niederschlag [mm]</Label>
        <Direction>NEGATIVE</Direction>
        <Position>RIGHT</Position>
        <PreferredAdjustment before="0" range="50" after="50" />
        <RendererRef ref="AutoNumberAxisRenderer" />
      </Axis>
      <Axis id="W_NN" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
        <Provider epid="de.openali.odysseus.chart.ext.axis.provider.GenericLinear" />
        <Label>Wasserstand [mNN]</Label>
        <Direction>POSITIVE</Direction>
        <Position>LEFT</Position>
        <PreferredAdjustment before="15" range="75" after="10" />
        <RendererRef ref="AutoNumberAxisRenderer" />
      </Axis>
      <Axis id="V" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
        <Provider epid="de.openali.odysseus.chart.ext.axis.provider.GenericLinear" />
        <Label>Volumen [hm³]</Label>
        <Direction>POSITIVE</Direction>
        <Position>LEFT</Position>
        <PreferredAdjustment before="15" range="75" after="10" />
        <RendererRef ref="AutoNumberAxisRenderer" />
      </Axis>
      <Axis id="date" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
        <Provider epid="de.openali.odysseus.chart.ext.axis.provider.GenericLinear" />
        <Label>Datum</Label>
        <Direction>POSITIVE</Direction>
        <Position>BOTTOM</Position>
        <PreferredAdjustment before="0" range="1" after="0" />
        <RendererRef ref="DateAxisRenderer" />
      </Axis>
      <ScreenAxis id="screenAxisX">
        <Provider epid="de.openali.odysseus.chart.ext.axis.provider.GenericLinear" />
        <Position>BOTTOM</Position>
      </ScreenAxis>
      <ScreenAxis id="screenAxisY">
        <Provider epid="de.openali.odysseus.chart.ext.axis.provider.GenericLinear" />
        <Position>LEFT</Position>
      </ScreenAxis>
    </Mappers>
    <Renderers>
      <AxisRenderer id="NumberAxisRenderer">
        <Provider epid="de.openali.odysseus.chart.ext.base.axisrenderer.provider.GenericNumberAxisRendererProvider">
          <Parameters>
            <Parameter name="inset_tick" value="2" />
            <Parameter name="inset_label" value="2" />
            <Parameter name="tick_length" value="2" />
            <Parameter name="hide_cut" value="true" />
            <Parameter name="gap" value="0" />
            <Parameter name="tick_label_formater" value="%.0f" />
          </Parameters>
        </Provider>
        <Styles>
          <StyleReference>urn:org:kalypso:zml:ui:diagramm:global#axisLabel</StyleReference>
          <StyleReference>urn:org:kalypso:zml:ui:diagramm:global#tickLabel</StyleReference>
        </Styles>
      </AxisRenderer>
      <AxisRenderer id="AutoNumberAxisRenderer">
        <Provider epid="de.openali.odysseus.chart.ext.base.axisrenderer.provider.GenericNumberAxisRendererProvider">
          <Parameters>
            <Parameter name="inset_tick" value="2" />
            <Parameter name="inset_label" value="2" />
            <Parameter name="tick_length" value="2" />
            <Parameter name="hide_cut" value="true" />
            <Parameter name="gap" value="0" />
          </Parameters>
        </Provider>
        <Styles>
          <StyleReference>urn:org:kalypso:zml:ui:diagramm:global#axisLabel</StyleReference>
          <StyleReference>urn:org:kalypso:zml:ui:diagramm:global#tickLabel</StyleReference>
        </Styles>
      </AxisRenderer>
      <AxisRenderer id="DateAxisRenderer">
        <Provider epid="de.openali.odysseus.chart.ext.base.axisrenderer.provider.GenericCalendarAxisRendererProvider">
          <Parameters>
            <Parameter name="inset_tick" value="2" />
            <Parameter name="inset_label" value="2" />
            <Parameter name="tick_length" value="2" />
            <Parameter name="min_tick_interval" value="86400000" />
            <Parameter name="hide_cut" value="true" />
            <Parameter name="gap" value="0" />
            <Parameter name="date-format" value="dd.MM.&#10;HH:mm" />
            <Parameter name="label_position" value="INTERVALL_CENTERED" />
          </Parameters>
        </Provider>
        <Styles>
          <StyleReference>urn:org:kalypso:zml:ui:diagramm:global#axisLabel</StyleReference>
          <StyleReference>urn:org:kalypso:zml:ui:diagramm:global#tickLabel</StyleReference>
        </Styles>
      </AxisRenderer>
    </Renderers>
  </Chart>
</ChartConfiguration>