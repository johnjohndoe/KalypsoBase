      <provider
            class="org.kalypso.chart.ext.base.axisrenderer.provider.GenericNumberAxisRendererProvider"
            id="org.kalypso.chart.ext.test.axisrenderer.provider.GenericNumberAxisRendererProvider">
      </provider>
