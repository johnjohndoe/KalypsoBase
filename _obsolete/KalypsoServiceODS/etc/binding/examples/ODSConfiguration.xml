     <Axis name="Width_Axis">
          <label>Mein Breiten-Achsen-Label</label>
          <provider id="org.kalypso.axisprovider.Number" />
          <direction>POSITIVE</direction>
          <position>BOTTOM</position>
          <minVal>0</minVal>
          <maxVal>100</maxVal>
     </Axis>
     <Axis name="Height_Axis">
          <label>Mein HÃ¶hen-Achsen-Label</label>
          <provider id="org.kalypso.axisprovider.Number" />
          <direction>POSITIVE</direction>
          <position>LEFT</position>
          <minVal>0</minVal>
          <maxVal>100</maxVal>
          <parameters>
               <Parameter name="" value="" />
          </parameters>
     </Axis>
     
     <Axis name="Wasserstand_Axis">
          <label>Wasserstand</label>
          <provider id="org.kalypso.axisprovider.Number" />
          <direction>POSITIVE</direction>
          <position>LEFT</position>
          <minVal>0</minVal>
          <maxVal>200</maxVal>
     </Axis>
     <Axis name="Datum_Axis">
          <label>Datum</label>
          <provider id="org.kalypso.axisprovider.Calendar" />
          <direction>POSITIVE</direction>
          <position>BOTTOM</position>
          <minVal>NOW-14d</minVal>
          <maxVal>NOW+14d</maxVal>
     </Axis>
     
     
     
